# Production Environment Variables for OpenIDX Admin Console
# Deployed at openidx.tdv.org

# API Configuration
VITE_API_URL=https://openidx.tdv.org

# OAuth/OIDC Configuration
VITE_OAUTH_URL=https://openidx.tdv.org
VITE_OAUTH_CLIENT_ID=admin-console

# Application Configuration
VITE_APP_NAME=OpenIDX
VITE_APP_ENV=production

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_AUDIT_LOG=true
VITE_ENABLE_MFA=true

# Session Configuration
VITE_SESSION_TIMEOUT_MINUTES=60
VITE_MAX_CONCURRENT_SESSIONS=5

# Branding
VITE_PRIMARY_COLOR=#3b82f6
VITE_SUPPORT_EMAIL=support@tdv.org
