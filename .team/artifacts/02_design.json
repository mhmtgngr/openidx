ITH TIME ZONE,\n  created_by UUID REFERENCES users(id),\n  metadata JSONB DEFAULT '{}'\n); CREATE INDEX idx_ai_identities_service_account ON ai_identities(service_account_id); CREATE INDEX idx_ai_identities_status ON ai_identities(status); CREATE INDEX idx_ai_identities_type ON ai_identities(agent_type);"},{"file":"migrations/20250225_create_ai_security_events_table.up.sql","sql":"CREATE TABLE ai_security_events (\n  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n  ai_identity_id UUID REFERENCES ai_identities(id) ON DELETE SET NULL,\n  event_type VARCHAR(50) NOT NULL CHECK (event_type IN ('prompt_injection', 'data_exfiltration', 'unauthorized_access', 'rate_limit_exceeded', 'anomalous_behavior')),\n  severity VARCHAR(20) NOT NULL CHECK (severity IN ('low', 'medium', 'high', 'critical')),\n  request_data JSONB,\n  detection_method VARCHAR(50) NOT NULL,\n  blocked BOOLEAN NOT NULL DEFAULT false,\n  detected_at TIMESTAMP WITH TIME ZONE DEFAULT NOW(),\n  reviewed_by UUID REFERENCES users(id),\n  reviewed_at TIMESTAMP WITH TIME ZONE,\n  resolution VARCHAR(100),\n  false_positive BOOLEAN DEFAULT false\n); CREATE INDEX idx_ai_security_events_ai_identity ON ai_security_events(ai_identity_id); CREATE INDEX idx_ai_security_events_type_severity ON ai_security_events(event_type, severity); CREATE INDEX idx_ai_security_events_detected_at ON ai_security_events(detected_at);"},{"file":"migrations/20250225_create_passwordless_credentials_table.up.sql","sql":"CREATE TABLE passwordless_credentials (\n  id UUID PRIMARY KEY DEFAULT gen_random_uuid(),\n  user_id UUID NOT NULL REFERENCES users(id) ON DELETE CASCADE,\n  credential_type VARCHAR(20) NOT NULL CHECK (credential_type IN ('passkey', 'webauthn', 'magic_link', 'biometric')),\n  credential_id VARCHAR(255) UNIQUE NOT NULL,\n  public_key TEXT NOT NULL,\n  counter BIGINT DEFAULT 0,\n  transports TEXT[],\n  aaguid UUID,\n  attestation_type VARCHAR(50),\n  trustworthy BOOLEAN DEFAULT true,\n  backup_eligible BOOLEAN DEFAULT false,\n  