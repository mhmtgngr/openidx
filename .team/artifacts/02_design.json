')),\n  status VARCHAR(20) NOT NULL DEFAULT 'open' CHECK (status IN ('open', 'investigating', 'resolved', 'false_positive')),\n  detection_evidence JSONB NOT NULL,\n  quarantine_actions JSONB DEFAULT '[]',\n  remediation_steps JSONB DEFAULT '[]',\n  detected_at TIMESTAMP WITH TIME ZONE DEFAULT NOW(),\n  resolved_at TIMESTAMP WITH TIME ZONE,\n  resolved_by UUID REFERENCES users(id),\n  incident_id UUID,\n  related_alerts UUID[] DEFAULT ARRAY[]::UUID[],\n  notifications_sent TEXT[] DEFAULT ARRAY[]::TEXT[]\n); CREATE INDEX idx_breach_alerts_user ON identity_breach_alerts(affected_user_id); CREATE INDEX idx_breach_alerts_status ON identity_breach_alerts(status); CREATE INDEX idx_breach_alerts_severity ON identity_breach_alerts(severity); CREATE INDEX idx_breach_alerts_detected_at ON identity_breach_alerts(detected_at);"}],"api_contracts":[{"method":"POST","path":"/api/v1/admin/ai-identities","request":{"agent_type":"semi-autonomous","name":"CustomerServiceBot","permissions":["read:customers","write:tickets"],"rate_limit_per_minute":120},"response":{"id":"uuid","name":"CustomerServiceBot","agent_type":"semi-autonomous","api_key":"sk-...","status":"active","created_at":"2025-02-25T10:00:00Z"},"status_codes":[201,400,401,403]},{"method":"GET","path":"/api/v1/admin/ai-identities","response":{"identities":[{"id":"uuid","name":"Bot","agent_type":"autonomous","status":"active","last_used_at":"2025-02-25T10:00:00Z"}],"total":10,"page":1,"per_page":20},"status_codes":[200,401,403]},{"method":"GET","path":"/api/v1/admin/ai-identities/:id","response":{"id":"uuid","name":"Bot","agent_type":"semi-autonomous","permissions":{},"rate_limit_per_minute":60,"status":"active","created_at":"2025-02-25T10:00:00Z","last_used_at":"2025-02-25T11:30:00Z","session_tokens":[]},"status_codes":[200,404,401,403]},{"method":"PUT","path":"/api/v1/admin/ai-identities/:id","request":{"name":"Updated Name","permissions":["new:permission"],"rate_limit_per_minute":100},"response":{"id":"uuid","name":"Update