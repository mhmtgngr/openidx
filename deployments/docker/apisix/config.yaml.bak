apisix:
  node_listen: 9080
  enable_ipv6: false
  enable_admin: false
  ssl:
    enable: true
    listen:
      - port: 9443

deployment:
  role: traditional
  role_traditional:
    config_provider: yaml
    yaml:
      default_yaml_config:
        - /usr/local/apisix/conf/apisix.yaml

plugin_attr:
  prometheus:
    export_addr:
      ip: 0.0.0.0
      port: 9091
